<h1>つぶやき一覧</h1>
<table>
  <%= @blogs.each do |blog| %>
    <tr>
    <td><%= blog.content %></td>
    <td><%= link_to '閲覧', blog_path(blog.id) %></td>
    <td><%= link_to '編集', edit_blog_path(blog.id) %></td>
    </tr>
   <% end %>
</table>
<br>
<%= link_to '新しいつぶやきを投稿する', new_blog_path %>
